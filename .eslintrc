{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "@vue/eslint-config-typescript"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "vue/no-setup-props-destructure": "off"
  },
  "globals": {
    "$ref": "readonly",
    "$computed": "readonly",
    "$shallowRef": "readonly",
    "$customRef": "readonly",
    "$toRef": "readonly",
    "$": "readonly",
    "$$": "readonly"
  }
}